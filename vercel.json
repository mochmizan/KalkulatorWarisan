{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "routes": [
      {
        "src": "/api/index.php",
        "dest": "/api/index.php"
      },
      {
        "src": "/style.css",
        "dest": "/public/style.css"
      },
      {
        "src": "/assets/img/(.*).png",
        "dest": "/public/assets/img/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
}
